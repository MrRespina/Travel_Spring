<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.travel.main.placeSave.PlaceSaveMapper">

	<insert id="insertSave" parameterType="com.travel.main.placeSave.Draftviews">
		insert into draftviews (f_id,photo, name, category, addr, lat, lng) 
			values (#{f_id},#{photo}, #{name}, #{category}, #{addr}, #{lat}, #{lng})
	</insert>
	
	<select id="getAllSave" resultType="com.travel.main.placeSave.Draftviews">
		select * from draftviews
	</select>
	
	<select id="searchTestName" resultType="com.travel.main.placeSave.Draftviews" parameterType="com.travel.main.placeSave.Draftviews">
		select * from draftviews where name like concat('%', #{name}, '%')
	</select>
	
	<delete id="delSave" parameterType="com.travel.main.placeSave.Draftviews">
		delete from draftviews where id = #{id}
	</delete>
	
</mapper>